{
  // Latex Workshop
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.outDir": "%DIR%/build",
  // Terminal
  "terminal.integrated.shell.windows": "D:\\Applications\\PowerShell\\7\\pwsh.exe",
  // Git settings
  "git.autofetch": true,
  "git.confirmSync": false,
  // Editor
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.fontFamily": "Comic Code, monospace",
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  // Powershell
  "powershell.powerShellAdditionalExePaths": [
    {
      "exePath": "D:\\Applications\\PowerShell\\7\\pwsh.exe",
      "versionName": "7.0.1"
    }
  ],
  "powershell.powerShellDefaultVersion": "7.0.1",
  // C-Cpp
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  // CMake
  "cmake.cmakePath": "cmake",
  "cmake.configureOnOpen": true,
  "cmake.configureArgs": [
    "-DBoost_INCLUDE_DIR=D:/Programming/Boost/1.73.0"
  ],
  // Latex Workshop
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "lualatexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-lualatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "latexmk_rconly",
      "command": "latexmk",
      "args": [
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
        "%DOCFILE%"
      ],
      "env": {}
    },
    {
      "name": "rnw2tex",
      "command": "Rscript",
      "args": [
        "-e",
        "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
      ],
      "env": {}
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.colorTheme": "One Dark Pro",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "window.zoomLevel": 0,
  "sqltools.useNodeRuntime": true,
  "sqltools.connections": [
    {
      "database": "D:\\Work\\neobot\\main_system-2020-06-04-14-44-46.sqlite",
      "driver": "SQLite",
      "name": "KWZ",
      "previewLimit": 50
    }
  ],
  "python.defaultInterpreterPath": "D:\\Programming\\Miniconda\\3\\python.exe",
}